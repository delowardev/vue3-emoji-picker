  "repository": {
    "type": "git",
    "url": "git+https://github.com/delowardev/vue3-emoji-picker"
  },
  "bugs": {
    "url": "https://github.com/delowardev/vue3-emoji-picker/issues"
  },
  "homepage": "https://github.com/delowardev/vue3-emoji-picker/",
  "keywords": [
    "vue3-emoji-picker",
